<script type="text/javascript">
    
    document.addEventListener("DOMContentLoaded", function () {
        var ele = document.getElementById(localStorage.getItem("mySession"));
        if (ele == null) {
            document.getElementById("__dash").classList.add("active-item");
            document.getElementById("__dash").children[0].children[0].style = "color:#e6c337 !important";
        } else {
            link = document.getElementsByClassName("sidebar-link");
            for (var i = 0; i < link.length; i++) {
                $(link[i]).css("background", "none");
                $(link[i]).css("border", "none");
            }
           
            $(ele).css("background", "rgb(249 245 230 / 0.68)");
            $(ele).css("border-left", "5px solid #e6c337");
            ele.children[0].children[0].style = "color:#e6c337 !important";

            
        }
       
        localStorage.removeItem("mySession");
    });
    function sleep(delay) {
        var start = new Date().getTime();
        while (new Date().getTime() < start + delay);
    }
    $(".sidebar-link").click(function () {
        localStorage.setItem("mySession", $(this).prop("id"));
        

    });
</script>