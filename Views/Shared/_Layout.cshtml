<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]-post office</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <partial name="Partials/_ClientStyles"/>
    @if(ViewData["Title"].ToString() == "Profile"){
        RenderSection("Styles");
    }
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="../../css/client/responsive-layout.css">
    <link rel="stylesheet" href="../../css/client/post-office.css">
</head>

<body class="home page-template-default page page-id-6439 theme-moversco woocommerce-no-js woocommerce woocommerce-page woocommerce-js 
            tribe-no-js themestek-headerstyle-6 themestek-wide themestek-header-overlay themestek-page-full-width themestek-empty-sidebar 
            wpb-js-composer js-comp-ver-6.6.0 vc_responsive single-themestek-service">
    <div id="themestek-home"></div>    
    <!-- Body -->
    <div class="main-holder">
        <div id="page" class="hfeed site">
            <!--Header-->
            <partial name="Partials/_ClientHeader"/>		
            <!--End Header-->    
            @RenderBody()
            <!-- Footer -->
            <partial name="Partials/_ClientFooter"/>
            <!-- End Footer ---------------------------------------------------------------------------------------->
        </div><!-- #page .site -->
    </div><!-- .main-holder -->
    <!-- End Body ------------------------------------------------------------------------------------------>


    <!-- To Top -->
    <a id="totop" href="#top"><i class="themestek-moversco-icon-angle-up"></i></a>
    <!-- script -->
    <partial name="Partials/_ClientScripts"/>
    <!-- end script -->
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
